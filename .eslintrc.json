{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "globals": {
  },
  "extends": [
    "airbnb/base",
    "plugin:react/recommended"
  ],
  "plugins": [
    "flowtype",
    "react"
  ],
  "settings": {
  },
  "rules": {
    "array-bracket-spacing": 0,
    "arrow-body-style": [1, "always"],
    "comma-dangle": 0,
    "computed-property-spacing": 0,
    "consistent-return": 0,
    "eqeqeq": 0,
    "func-names": 0,
    "global-require": 0,
    "import/default": 0,
    "import/export": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/named": 0,
    "import/namespace": 0,
    "key-spacing": 0,
    "max-len": 0,
    "no-console": 0,
    "no-global-assign": 0,
    "no-unsafe-negation": 0,
    "no-duplicate-imports": 0,
    "no-irregular-whitespace": 0,
    "no-multi-spaces": 0,
    "no-multiple-empty-lines": 0,
    "no-continue": 0,
    "no-nested-ternary": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": ["error", "WithStatement"],
    "no-underscore-dangle": 0,
    "no-trailing-spaces": 1,
    "object-curly-spacing": 0,
    "object-shorthand": 0,
    "one-var": 0,
    "prefer-template": 0,
    "prefer-arrow-callback": 0,
    "space-in-parens": 0,
    "indent": [2, 2, {"SwitchCase": 1, "VariableDeclarator": 1}],
    "import/no-duplicates": 0,
    "flowtype/require-parameter-type": 0,
    "flowtype/require-return-type": [0, "always", {"annotateUndefined": "never"}],
    "flowtype/space-after-type-colon": [0, "always"],
    "flowtype/space-before-type-colon": [0, "never"],
    "flowtype/type-id-match": [1, "^([A-Z][a-zA-Z0-9]+)$"],
    "no-undef": 1,
    "no-use-before-define": 2,
    "no-console": 2,
    "no-debugger": 2,
    "no-prototype-builtins": 1,
    "padded-blocks": 0,
    "space-before-function-paren": 0,
    "space-unary-ops": 1
  }
}
